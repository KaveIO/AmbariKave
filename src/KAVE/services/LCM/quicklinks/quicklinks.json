{
  "name": "default",
  "description": "default quick links configuration",
  "configuration": {
    "protocol":
    {
      "type":"https",
      "checks":[
        {
          "property":"LCM_SecureUI_Port",
          "desired":"EXIST",
          "site":"lcm_application"
        },
        {
          "property":"LCM_UI_URL",
          "desired":"EXIST",
          "site":"lcm_application"
        }
      ]
    },
    "links": [
      {
        "name": "lcm_server_link",
        "label": "LCM Web Interface",
        "requires_user_name": "false",
        "component_name": "LCM_UI",
        "url":"%@://%@:%@",
        "port":{
          "https_property": "LCM_SecureUI_Port",
          "https_default_port": "4444",
          "regex": "^(\\d+)$",
          "site": "lcm_application"
        }
      }
    ]
  }
}